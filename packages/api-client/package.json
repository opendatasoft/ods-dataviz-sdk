{
    "name": "@opendatasoft/api-client",
    "version": "0.4.4",
    "license": "MIT",
    "author": "opendatasoft",
    "homepage": "https://github.com/opendatasoft/ods-dataviz-sdk",
    "description": "Opendatasoft's API Client.",
    "keywords": [
        "Opendatasoft",
        "ODS",
        "API Client",
        "Dataviz",
        "JavaScript",
        "TypeScript"
    ],
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist/api-client.esm.js",
    "unpkg": "umd/opendatasoft.apiclient.umd.js",
    "files": [
        "dist",
        "src",
        "umd"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "watch": "rollup --config --watch",
        "build": "rollup --config",
        "docs": "typedoc",
        "test": "jest",
        "lint": "run-p lint:*:check -c",
        "lint:fix": "run-p lint:*:fix -c",
        "lint:eslint:check": "eslint src test",
        "lint:prettier:check": "prettier --check src test",
        "lint:eslint:fix": "eslint src test --fix",
        "lint:prettier:fix": "prettier src test --write",
        "prepare": "rimraf dist && rollup --config",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "always"
    },
    "size-limit": [
        {
            "path": "dist/api-client.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/api-client.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.19.0",
        "@babel/preset-env": "^7.19.0",
        "@babel/preset-typescript": "^7.18.6",
        "@jest/globals": "^29.0.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^10.0.0",
        "@rollup/plugin-replace": "^3.0.1",
        "@rollup/plugin-typescript": "^6.1.0",
        "@size-limit/preset-small-lib": "^7.0.5",
        "@types/jest": "^29.0.0",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "autoprefixer": "^10.4.8",
        "babel-jest": "^29.0.2",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.0.2",
        "jest-environment-jsdom": "^29.0.2",
        "jest-fetch-mock": "^3.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.16",
        "prettier": "2.4.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.33.1",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-svelte": "^6.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-visualizer": "^4.2.0",
        "size-limit": "^7.0.5",
        "tslib": "^2.0.3",
        "typedoc": "^0.22.10",
        "typedoc-plugin-markdown": "^3.11.9",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "immutability-helper": "^3.1.1"
    }
}
