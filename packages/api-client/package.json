{
    "name": "@opendatasoft/api-client",
    "version": "0.4.4",
    "license": "MIT",
    "author": "opendatasoft",
    "homepage": "https://github.com/opendatasoft/ods-dataviz-sdk",
    "description": "Opendatasoft's API Client.",
    "keywords": [
        "Opendatasoft",
        "ODS",
        "API Client",
        "Dataviz",
        "JavaScript",
        "TypeScript"
    ],
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist/api-client.esm.js",
    "unpkg": "umd/opendatasoft.apiclient.umd.js",
    "files": [
        "dist",
        "src",
        "umd"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "watch": "tsdx watch --verbose",
        "build": "tsdx build --format cjs,esm,umd --name 'opendatasoft.apiClient'",
        "docs": "typedoc",
        "test": "tsdx test",
        "lint": "run-p lint:*:check -c",
        "lint:fix": "run-p lint:*:fix -c",
        "lint:eslint:check": "eslint src test",
        "lint:prettier:check": "prettier --check src test",
        "lint:eslint:fix": "eslint src test --fix",
        "lint:prettier:fix": "prettier src test --write",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "test:watch": "tsdx test --watch",
        "test:coverage": "tsdx test --coverage"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "always"
    },
    "size-limit": [
        {
            "path": "dist/api-client.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/api-client.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@jest/globals": "^29.0.0",
        "@size-limit/preset-small-lib": "^7.0.5",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest-fetch-mock": "^3.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "2.4.1",
        "size-limit": "^7.0.5",
        "tsdx": "^0.14.1",
        "tslib": "^2.0.3",
        "typedoc": "^0.22.10",
        "typedoc-plugin-markdown": "^3.11.9",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "immutability-helper": "^3.1.1"
    }
}
