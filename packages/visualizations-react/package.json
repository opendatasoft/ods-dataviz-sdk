        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^4.3.7",
        "jest": "^29.0.2",
        "jest-canvas-mock": "^2.3.0",
        "jest-environment-jsdom": "^29.0.2",
        "jest-fetch-mock": "^3.0.3",
        "npm-run-all": "^4.1.5",
